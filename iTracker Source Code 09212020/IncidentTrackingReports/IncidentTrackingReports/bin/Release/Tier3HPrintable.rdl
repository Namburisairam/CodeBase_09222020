    <DataSet Name="GetYear">
      <Query>
        <DataSourceName>AutoIncidentTracking</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@type">
            <Value>=Parameters!type.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@area">
            <Value>=Parameters!area.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          SELECT DISTINCT [Year]
          FROM (
          SELECT DISTINCT YEAR(LocalActivityPerformedDateTime) [Year]
          FROM AUTO_IncidentTrack.dbo.Incidents
          WHERE @type = 'PCS' AND [ManufacturingAreaId] = @area
          UNION
          SELECT DISTINCT YEAR(LocalActivityPerformedDateTime) [Year]
          FROM MES_IncidentTrack.dbo.Incidents
          WHERE @type = 'MES' AND [ManufacturingAreaId] = @area
          ) q
          ORDER BY 1
        </CommandText>
      </Query>
      <Fields>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="GetMonth">
      <Query>
        <DataSourceName>AutoIncidentTracking</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@type">
            <Value>=Parameters!type.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@area">
            <Value>=Parameters!area.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="@year">
            <Value>=Parameters!year.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
          SELECT DISTINCT [Month], [MonthName]
          FROM (
          SELECT DISTINCT Month(LocalActivityPerformedDateTime) [Month],
          FORMAT(LocalActivityPerformedDateTime, 'MMM', 'en-US') [MonthName]
          FROM AUTO_IncidentTrack.dbo.Incidents
          WHERE @type = 'PCS' AND [ManufacturingAreaId] = @area
          AND Year(LocalActivityPerformedDatetime) = @year
          UNION
          SELECT DISTINCT Month(LocalActivityPerformedDateTime) [Month],
          FORMAT(LocalActivityPerformedDateTime, 'MMM', 'en-US') [MonthName]
          FROM MES_IncidentTrack.dbo.Incidents
          WHERE @type = 'MES' AND [ManufacturingAreaId] = @area
          AND Year(LocalActivityPerformedDatetime) = @year
          ) q
          ORDER BY 1
        </CommandText>
      </Query>
      <Fields>
        <Field Name="Month">
          <DataField>Month</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="MonthName">
          <DataField>MonthName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
